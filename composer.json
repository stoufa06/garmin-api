{
    "name": "trainerplan/garmin-api",
    "description": "PHP Garmin API library : Connect garmin api using app access tokens, use Activity API and Training API.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "league/oauth1-client": "1.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "TrainerPlan"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Garmin\\": "src/"
        }
    },
    "keywords": [
        "garmin-api",
        "garmin-connect-api",
        "garmin-activity-api",
        "garmin-training-api",
        "garmin-php-api"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
